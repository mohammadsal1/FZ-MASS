------------------------------------------------------------------------------THE CODE-----------------------------------------------------------------------------------------------
--------------------------------------------------------------------------------GPU--------------------------------------------------------------------------------------------------
cd ~/Documents/ACC_Development/isaac_ros_common
./scripts/run_dev.sh ~/Documents/ACC_Development/Development

cd /workspaces/isaac_ros-dev
colcon build --packages-select qcar2_interfaces
colcon build
source /workspaces/isaac_ros-dev/install/setup.bash
cd /workspaces/isaac_ros-dev/src/acc_2026_project/acc_2026_project
python3 osman_dembiala.py






