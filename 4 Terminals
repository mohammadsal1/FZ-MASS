------------------------------------------------------------------------------ROS 2 NODES-------------------------------------------------------------------------------------------
---------------------------------------------------------------------------------CPU-------------------------------------------------------------------------------------------------
"""
sudo docker run -it --rm \
  --privileged \
  --network host \
  --user 1000:1000 \
  -e DISPLAY=$DISPLAY \
  -v /tmp/.X11-unix:/tmp/.X11-unix \
  -v /home/mohammadsal/Documents/ACC_Development/isaac_ros_common:/workspaces/isaac_ros-dev \
  isaac_ros_dev-x86_64 /bin/bash
"""
cd /workspaces/isaac_ros-dev
source install/setup.bash
export FASTRTPS_DEFAULT_PROFILES_FILE=/workspaces/isaac_ros-dev/disable_shm.xml
ros2 launch qcar2_nodes qcar2_virtual_launch.py




